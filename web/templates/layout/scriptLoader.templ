package layoutTemplates

import (
	"mael/web/templates/components/ui/dialog"
	"mael/web/templates/components/ui/accordion"
	"mael/web/templates/components/ui/selectBox"
	"mael/web/templates/components/ui/tabs"
	"mael/web/templates/components/ui/toast"
	"mael/web/templates/components/ui/input"
	"mael/web/templates/components/ui/animation"
	"mael/web/templates/components/ui/dropDownMenu"
	toggle "mael/web/templates/components/ui/switchToggle"
)

templ Scripts(){
    @dialog.DialogScript()
    @accordion.AccordionScript()
    @selectBox.SelectScript()
    @tabs.TabsScript()
    @toggle.ToggleScript()
    @toast.AlertScript()
    @input.InputScript()
    @dropDownMenu.DropDownMenuScript()
    @animation.OnIntersectScript()
}
